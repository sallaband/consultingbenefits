Enter your information
<BR>
  <%= form_for @contacts do |f| %>
  First Name: <%= f.text_field :first_name %><BR>
  Last Name: <%= f.text_field :last_name %>
  Title:  <%= f.text_field :contact_title %><BR>
  Company Name:  <%= f.text_field :company %><BR>
  Phone: <%= f.text_field :phone %><BR>
  Email: <%= f.text_field :email %><BR>
  Question: <%= f.text_field :question %><BR>
  Other Concerns: <%= f.text_field :other_topics %><BR>
  The best way to reach you: <%= f.text_field :best_method %><BR>
  The best time to contact you: <%= f.text_field :best_time %><BR>
  <%= f.submit %>
  <% end %>
